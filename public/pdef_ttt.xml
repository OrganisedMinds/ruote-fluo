    on-error="emergency"
    on-cancel="decommission"
  >
    <participant ref="Reception" />
    <concurrence>
      <participant ref="Ted" />
      <participant ref="Tang" />
    </concurrence>
    <error if="surf">nada</error>
    <sleep>3h</sleep>
    <participant ref="Reception" />
  </sequence>
</process-definition>
